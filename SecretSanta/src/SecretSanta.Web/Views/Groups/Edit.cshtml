@model GroupViewModel

<form asp-controller="Groups" asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    @await Html.PartialAsync("_CreateOrEdit")
    <button class="generalButton">Update</button>
    <a class="underlinedText inline-block" asp-controller="Groups" asp-action="Index">Cancel</a>
</form>